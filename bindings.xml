<Bindings>
	<Binding name="Toggle Targets +" header="PROPHETIC" category="ADDONS">
		Prophetic_ToggleTargetMode()
	</Binding>
	<Binding name="Toggle Targets -" category="ADDONS">
		Prophetic_ToggleTargetModeReverse()
	</Binding>
	<Binding name="Set Targets to 1" category="ADDONS">
		Prophetic_SetTargetMode(1)
	</Binding>
	<Binding name="Set Targets to 2" category="ADDONS">
		Prophetic_SetTargetMode(2)
	</Binding>
	<Binding name="Set Targets to 3" category="ADDONS">
		Prophetic_SetTargetMode(3)
	</Binding>
	<Binding name="Set Targets to 4+" category="ADDONS">
		Prophetic_SetTargetMode(4)
	</Binding>
</Bindings>
