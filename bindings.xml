<Bindings>
	<Binding name="PROPHETIC_TARGETMORE" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_ToggleTargetMode()
	</Binding>
	<Binding name="PROPHETIC_TARGETLESS" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_ToggleTargetModeReverse()
	</Binding>
	<Binding name="PROPHETIC_TARGET1" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_SetTargetMode(1)
	</Binding>
	<Binding name="PROPHETIC_TARGET2" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_SetTargetMode(2)
	</Binding>
	<Binding name="PROPHETIC_TARGET3" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_SetTargetMode(3)
	</Binding>
	<Binding name="PROPHETIC_TARGET4" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_SetTargetMode(4)
	</Binding>
	<Binding name="PROPHETIC_TARGET5" category="BINDING_CATEGORY_PROPHETIC">
		Prophetic_SetTargetMode(5)
	</Binding>
</Bindings>
